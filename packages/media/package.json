{
  "name": "@polar/media",
  "version": "1.0.0",
  "description": "Media handling and file upload utilities for Polar",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint src --fix",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@polar/services": "workspace:*",
    "@polar/types": "workspace:*",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "imagekit": "^6.0.0",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@polar/typescript-config": "workspace:*",
    "@types/node": "^22.16.5",
    "eslint": "^9.31.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "media",
    "file-upload",
    "imagekit",
    "multer",
    "polar"
  ],
  "author": "Davide Usberti <usbertibox@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "publishConfig": {
    "access": "restricted"
  }
}
